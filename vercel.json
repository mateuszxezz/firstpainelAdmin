{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [{
    "src": "/(.*)",
    "dest": "/api/index.php"
}],
    "env": {
        "APP_NAME": "Laravel",
